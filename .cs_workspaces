CS_LAST_WORKSPACE: blog
CS_WORKSPACE_BEGIN: blog
let SessionLoad = 1
if &cp | set nocp | endif
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
exe "cd " . escape(expand("<sfile>:p:h"), ' ')
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +38 source/_posts/Lets-Learn-Vim-the-smooth-way.md
argglobal
silent! argdel *
edit source/_posts/Lets-Learn-Vim-the-smooth-way.md
edit source/_posts/Lets-Learn-Vim-the-smooth-way.md
set splitbelow splitright
wincmd t
set winheight=1 winwidth=1
argglobal
let s:l = 30 - ((29 * winheight(0) + 19) / 39)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
30
normal! 050|
tabnext 1
if exists('s:wipebuf')
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 shortmess=I
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
CS_WORKSPACE_END: blog
